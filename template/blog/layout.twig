    <link rel="shortcut icon" href="../template/blog/images/favicon.ico">
    <link rel="apple-touch-icon" href="../template/blog/images/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="../template/blog/images/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="../template/blog/images/apple-touch-icon-114x114.png">

