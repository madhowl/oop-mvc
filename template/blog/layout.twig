    <link rel="stylesheet" type="text/css" href="../template/blog/css/vendor/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="../template/blog/css/vendor/owl.carousel.min.css">
    <link rel="stylesheet" type="text/css" href="../template/blog/css/vendor/owl.theme.default.min.css">
    <link rel="stylesheet" type="text/css" href="../template/blog/css/vendor/magnific-popup.css">
    <link rel="stylesheet" type="text/css" href="../template/blog/css/vendor/animate.min.css">

    <!-- ==============================================
